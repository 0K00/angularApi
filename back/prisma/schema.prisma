// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Tasks {
  id String          @default(nanoid(8)) @id
  title String
  description String
  completed Boolean @default(false)
}